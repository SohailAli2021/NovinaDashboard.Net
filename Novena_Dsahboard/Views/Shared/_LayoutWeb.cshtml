
@inject SignInManager<IdentityUser> signInManager
<!DOCTYPE html> 
<html lang="zxx">
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
    <meta name="description" content="Orbitor,business,company,agency,modern,bootstrap4,tech,software">
    <meta name="author">

    <title>Novena- Health & Care Medical template</title>

    <!-- Favicon -->
    <link rel="shortcut icon" type="image/x-icon" href="~/Assets_Web/images/favicon.ico" />

    <!-- bootstrap.min css -->
    <link rel="stylesheet" href="~/Assets_Web/plugins/bootstrap/css/bootstrap.min.css">
    <!-- Icon Font Css -->
    <link rel="stylesheet" href="~/Assets_Web/plugins/icofont/icofont.min.css">
    <!-- Slick Slider  CSS -->
    <link rel="stylesheet" href="~/Assets_Web/plugins/slick-carousel/slick/slick.css">
    <link rel="stylesheet" href="~/Assets_Web/plugins/slick-carousel/slick/slick-theme.css">

    <!-- Main Stylesheet -->
    <link rel="stylesheet" href="~/Assets_Web/css/style.css">

</head>


<body id="top">

    <header>
        <div class="header-top-bar">
            <div class="container">
                <div class="row align-items-center">
                    <div class="col-lg-6">
                        <ul class="top-bar-info list-inline-item pl-0 mb-0">
                            <li class="list-inline-item"><a href="mailto:support@gmail.com"><i class="icofont-support-faq mr-2"></i>support@novena.com</a></li>
                            <li class="list-inline-item"><i class="icofont-location-pin mr-2"></i>Address Ta-134/A, New York, USA </li>
                        </ul>
                    </div>
                    <div class="col-lg-6">
                        <div class="text-lg-right top-right-bar mt-2 mt-lg-0">
                            <a href="tel:+23-345-67890">
                                <span>Call Now : </span>
                                <span class="h4">823-4565-13456</span>
                            </a>
                        </div>
                    </div>
                  
                </div>

                <div class="col-lg-12 p-0 justify-content-end">
                    <form asp-controller="HomeDB" asp-action="Product">
                        <span class="btn-group">
                            <input class="form-control p-4" type="text" name="SearchText" placeholder="Search Medicine Here" />
                            <input class="btn btn-light btn-round-full mb-3" name="SearchText" type="submit" style="height: 50px;" />
                        </span>
                    </form>
                </div>


               
            </div>
        </div>

        <nav class="navbar navbar-expand-lg navigation" id="navbar">
            <div class="container">
                <a class="navbar-brand" asp-action="Index" asp-controller="Home">
                    <img src="~/Assets_Web/images/logo.png" alt="" class="img-fluid">
                </a>

                <button class="navbar-toggler collapsed" type="button" data-toggle="collapse" data-target="#navbarmain" aria-controls="navbarmain" aria-expanded="false" aria-label="Toggle navigation">
                    <span class="icofont-navigation-menu"></span>
                </button>

                <div class="collapse navbar-collapse" id="navbarmain">
                    <ul class="navbar-nav ml-auto">
                        <li class="nav-item active">
                            <a class="nav-link" asp-action="Index" asp-controller="Home">Home</a>
                        </li>
                        <li class="nav-item"><a class="nav-link" asp-action="About" asp-controller="Home">About</a></li>
                        @*<li class="nav-item"><a class="nav-link" asp-action="Service" asp-controller="HomeDB">Services</a></li>*@
                        <li class="nav-item dropdown">
                            <a class="nav-link dropdown-toggle" id="dropdown05" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">Services <i class="icofont-thin-down"></i></a>
                            <ul class="dropdown-menu" aria-labelledby="dropdown05">
                                <li><a class="dropdown-item" asp-action="Service" asp-controller="HomeDB">Our Services</a></li>

                                <li><a class="dropdown-item" asp-action="Create" asp-controller="Appointments">Book An Appointment</a></li>
                                <li><a class="dropdown-item" asp-action="Department" asp-controller="HomeDB">Departments</a></li>
                                <li><a class="dropdown-item" asp-action="Doctor" asp-controller="HomeDB">Doctors</a></li>

                            </ul>
                        </li>

                        <li class="nav-item dropdown">
                            <a class="nav-link dropdown-toggle" href="department.html" id="dropdown02" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">Shop <i class="icofont-thin-down"></i></a>
                            <ul class="dropdown-menu" aria-labelledby="dropdown02">
                                <li><a class="dropdown-item" asp-action="Product" asp-controller="HomeDB">Medical</a></li>
                                <li><a class="dropdown-item" asp-action="Scientific" asp-controller="HomeDB">Scientific</a></li>
                            </ul>
                        </li>

                        @*<li class="nav-item dropdown">
                                <a class="nav-link dropdown-toggle" href="doctor.html" id="dropdown03" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">Doctors <i class="icofont-thin-down"></i></a>
                                <ul class="dropdown-menu" aria-labelledby="dropdown03">
                                    <li><a class="dropdown-item" href="doctor.html">Doctors</a></li>
                                    <li><a class="dropdown-item" href="doctor-single.html">Doctor Single</a></li>
                                    <li><a class="dropdown-item" asp-action="Create" asp-controller="Appointments">Appoinment</a></li>
                                </ul>
                            </li>*@

                        <li class="nav-item dropdown">
                            <a class="nav-link dropdown-toggle" id="dropdown05" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">Blog <i class="icofont-thin-down"></i></a>
                            <ul class="dropdown-menu" aria-labelledby="dropdown05">
                                <li><a class="dropdown-item" asp-action="Casestudy" asp-controller="HomeDB">Case Studies</a></li>

                                <li><a class="dropdown-item" asp-action="Event" asp-controller="HomeDB">Events</a></li>
                                <li><a class="dropdown-item" asp-action="Speaker" asp-controller="HomeDB">Speakers</a></li>

                            </ul>
                        </li>
                        <li class="nav-item"><a class="nav-link" asp-action="Create" asp-controller="Contacts">Contact</a></li>
                        <li class="nav-item"><a class="nav-link" asp-action="Create" asp-controller="Feedbacks">Feedback</a></li>
                        @if (signInManager.IsSignedIn(User) && User.IsInRole("Admin"))
                        {
                            <li class="nav-item"><a class="nav-link" asp-action="Index" asp-controller="HomeDB">Dashboard</a></li>

                        }
                        @if (signInManager.IsSignedIn(User))
                        {
                            <li class="nav-item">
                                <form method="post" asp-controller="Account" asp-action="Logout">
                                    <button type="submit" style="width:auto" class="nav-link btn btn-link py-0">
                                        Logout @User.Identity.Name
                                    </button>
                                </form>
                            </li>
                        }
                        else
                        {

                            <li class="nav-item dropdown">

                                <a class="nav-link dropdown-toggle" id="dropdown05" data-toggle="dropdown" aria-haspopup="true" aria-expanded="false">Profile<i class="icofont-thin-down"></i></a>
                                <ul class="dropdown-menu" aria-labelledby="dropdown05">
                                    <li><a class="dropdown-item" asp-area="" asp-controller="Account" asp-action="Login">Login</a></li>

                                    <li><a class="dropdown-item" asp-area="" asp-controller="Account" asp-action="Register">Register</a></li>

                                </ul>
                            </li>

                        }

                    </ul>
                </div>
            </div>
        </nav>



    </header>




    <!-- Slider Start -->


    <div>
        @RenderBody()
    </div>
    <!-- footer Start -->
    <footer class="footer section gray-bg">
        <div class="container">
            <div class="row">
                <div class="col-lg-4 mr-auto col-sm-6">
                    <div class="widget mb-5 mb-lg-0">
                        <div class="logo mb-4">
                            <img src="~/Assets_Web/images/logo.png" alt="" class="img-fluid">
                        </div>
                        <p>Tempora dolorem voluptatum nam vero assumenda voluptate, facilis ad eos obcaecati tenetur veritatis eveniet distinctio possimus.</p>

                        <ul class="list-inline footer-socials mt-4">
                            <li class="list-inline-item"><a href="https://www.facebook.com/themefisher"><i class="icofont-facebook"></i></a></li>
                            <li class="list-inline-item"><a href="https://twitter.com/themefisher"><i class="icofont-twitter"></i></a></li>
                            <li class="list-inline-item"><a href="https://www.pinterest.com/themefisher/"><i class="icofont-linkedin"></i></a></li>
                        </ul>
                    </div>
                </div>

                <div class="col-lg-2 col-md-6 col-sm-6">
                    <div class="widget mb-5 mb-lg-0">
                        <h4 class="text-capitalize mb-3">Department</h4>
                        <div class="divider mb-4"></div>

                        <ul class="list-unstyled footer-menu lh-35">
                            <li><a href="#">Surgery </a></li>
                            <li><a href="#">Wome's Health</a></li>
                            <li><a href="#">Radiology</a></li>
                            <li><a href="#">Cardioc</a></li>
                            <li><a href="#">Medicine</a></li>
                        </ul>
                    </div>
                </div>

                <div class="col-lg-2 col-md-6 col-sm-6">
                    <div class="widget mb-5 mb-lg-0">
                        <h4 class="text-capitalize mb-3">Support</h4>
                        <div class="divider mb-4"></div>

                        <ul class="list-unstyled footer-menu lh-35">
                            <li><a href="#">Terms & Conditions</a></li>
                            <li><a href="#">Privacy Policy</a></li>
                            <li><a href="#">Company Support </a></li>
                            <li><a href="#">FAQuestions</a></li>
                            <li><a href="#">Company Licence</a></li>
                        </ul>
                    </div>
                </div>

                <div class="col-lg-3 col-md-6 col-sm-6">
                    <div class="widget widget-contact mb-5 mb-lg-0">
                        <h4 class="text-capitalize mb-3">Get in Touch</h4>
                        <div class="divider mb-4"></div>

                        <div class="footer-contact-block mb-4">
                            <div class="icon d-flex align-items-center">
                                <i class="icofont-email mr-3"></i>
                                <span class="h6 mb-0">Support Available for 24/7</span>
                            </div>
                            <h4 class="mt-2"><a href="tel:+23-345-67890">Support@email.com</a></h4>
                        </div>

                        <div class="footer-contact-block">
                            <div class="icon d-flex align-items-center">
                                <i class="icofont-support mr-3"></i>
                                <span class="h6 mb-0">Mon to Fri : 08:30 - 18:00</span>
                            </div>
                            <h4 class="mt-2"><a href="tel:+23-345-67890">+23-456-6588</a></h4>
                        </div>
                    </div>
                </div>
            </div>

            <div class="footer-btm py-4 mt-5">
                <div class="row align-items-center justify-content-between">
                    <div class="col-lg-6">
                        <div class="copyright">
                            &copy; Copyright Reserved to <span class="text-color">Novena</span> by <a href="https://themefisher.com/" target="_blank">Themefisher</a>
                        </div>
                    </div>
                    <div class="col-lg-6">
                        <div class="subscribe-form text-lg-right mt-5 mt-lg-0">
                            <form action="#" class="subscribe">
                                <input type="text" class="form-control" placeholder="Your Email address">
                                <a href="#" class="btn btn-main-2 btn-round-full">Subscribe</a>
                            </form>
                        </div>
                    </div>
                </div>

                <div class="row">
                    <div class="col-lg-4">
                        <a class="backtop js-scroll-trigger" href="#top">
                            <i class="icofont-long-arrow-up"></i>
                        </a>
                    </div>
                </div>
            </div>
        </div>
    </footer>

    @RenderSection("Scripts", required: false)

    <!--
    Essential Scripts
    =====================================-->
    <!-- Main jQuery -->
    <script src="~/Assets_Web/plugins/jquery/jquery.js"></script>
    <!-- Bootstrap 4.3.2 -->
    <script src="~/Assets_Web/plugins/bootstrap/js/popper.js"></script>
    <script src="~/Assets_Web/plugins/bootstrap/js/bootstrap.min.js"></script>
    <script src="~/Assets_Web/plugins/counterup/jquery.easing.js"></script>
    <!-- Slick Slider -->
    <script src="~/Assets_Web/plugins/slick-carousel/slick/slick.min.js"></script>
    <!-- Counterup -->
    <script src="~/Assets_Web/plugins/counterup/jquery.waypoints.min.js"></script>

    <script src="~/Assets_Web/plugins/shuffle/shuffle.min.js"></script>
    <script src="~/Assets_Web/plugins/counterup/jquery.counterup.min.js"></script>
    <!-- Google Map -->
    <script src="~/Assets_Web/plugins/google-map/map.js"></script>
    <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyAkeLMlsiwzp6b3Gnaxd86lvakimwGA6UA&callback=initMap"></script>

    <script src="~/Assets_Web/js/script.js"></script>
    <script src="~/Assets_Web/js/contact.js"></script>

</body>
</html>
